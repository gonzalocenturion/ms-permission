{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Elasticsearch", "Serilog.Sinks.Async" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]    
  },
  "ConnectionStrings": {
    "Database": ""
  },
  "AllowedHosts": "*",
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200",
    "User": "elastic",
    "Password": "elastic"
  }
}
